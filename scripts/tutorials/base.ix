includable-chunk

pattern tutorial = {
    name: str
    id: str
    program: str
    output?: []str
}

pattern tutorial-series = {
    name: str
    id: str
    description?: str
    tutorials: []tutorial
}


fn get_order_string(index int){
    assert (index <= 99)
    if(index >= 10){
        return concat tostr(index)
    }
    return concat "0" tostr(index)
}